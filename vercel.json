{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.py"
    },
    {
      "src": "/analitica.html",
      "dest": "/analitica.html"
    },
    {
      "src": "/analytics_results.json",
      "dest": "/analytics_results.json"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
